<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="320.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tetris.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Canvas fx:id="mainCanvas" height="500.0" width="200.0" />

    <Label fx:id="welcomeText" />
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <HBox prefHeight="44.0" prefWidth="214.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
               <AnchorPane prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Button mnemonicParsing="false" prefHeight="37.0" prefWidth="107.0" text="Options" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <font>
                           <Font size="16.0" />
                        </font>
                     </Button>
                  </children>
               </AnchorPane>
               <AnchorPane prefHeight="200.0" prefWidth="200.0">
                  <children>
                      <Button onAction="#onHelloButtonClick" prefHeight="38.0" prefWidth="146.0" text="Start" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <font>
                           <Font size="16.0" />
                        </font>
                     </Button>
                  </children>
               </AnchorPane>
            </children>
         </HBox>
      </children>
   </AnchorPane>
</VBox>
